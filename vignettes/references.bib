  issue           = {12},
  keywords        = {Boston, epidemiology; Catheterization, Central Venous, adverse effects, utilization; Catheters, Indwelling, microbiology, utilization; Cohort Studies; Cross Infection, epidemiology, etiology; Health Maintenance Organizations; Hospital Information Systems; Hospitals, Teaching; Hospitals, Urban; Humans; Insurance Claim Reporting; Medical Records Systems, Computerized; Public Health Informatics; Reproducibility of Results; Sentinel Surveillance},
  nlm-id          = {8804099},
