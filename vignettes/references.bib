	Keywords        = {Adolescent; Brain Injuries, Traumatic, diagnosis, mortality; Child; Child, Preschool; Databases, Factual; Female; Hospital Mortality; Humans; Infant; Intracranial Pressure; Length of Stay, statistics & numerical data; Male; Monitoring, Physiologic, methods; Patient Discharge, statistics & numerical data; Propensity Score; Survival Rate; United States},
	Month           = {oct},
