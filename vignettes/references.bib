  Number        = {5},
  Pages         = {442--451},
  Revised       = {2017-03-05},
  Title         = {Development and Prospective Validation of Tools to Accurately Identify Neurosurgical and Critical Care Events in Children With Traumatic Brain Injury.},
  Volume        = {18},
